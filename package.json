{
  "name": "@cowtech/cera",
  "version": "0.2.0",
  "description": "CowTech Testing utilities.",
  "main": "./lib/index.js",
  "typings": "./lib/index",
  "types": "./lib/index",
  "files": [
    "lib",
    "types",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "lint": "tslint --project tsconfig.test.json -t stylish src/*.ts test/*.ts",
    "test": "jest test --coverage --coverageReporters=html --coverageReporters=text",
    "ci": "yarn lint",
    "prebuild": "rm -rf lib/* types/* && yarn lint",
    "build": "tsc -p .",
    "prepublishOnly": "yarn ci",
    "postpublish": "git push origin && git push origin -f --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cowtech/cera"
  },
  "keywords": [],
  "author": "Shogun <shogun@cowtech.it>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cowtech/cera/issues"
  },
  "homepage": "https://github.com/cowtech/cera",
  "dependencies": {
    "jest": "^24.1.0"
  },
  "devDependencies": {
    "@cowtech/tslint-config": "^5.12.4",
    "@types/got": "^9.4.1",
    "@types/jest": "^24.0.11",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  }
}
